export struct RichTextRun {
    text: string,
    bold: bool,
    italic: bool,
    underline: bool,
    font_size_px: int,
    font_family: string,
    color_mode: int, // 0 default, 1 palette, 2 rgba
    palette_color: int,
    color_rgba: color,
}

export struct RichTextLine {
    runs: [RichTextRun],
}

export struct RichTextBlock {
    plain_text: string,
    lines: [RichTextLine],
}

export struct TrackRowData {
    status: string,
    values: [string],
    rich_values: [RichTextBlock],
    album_art: image,
    has_album_art: bool,
    source_badge: string,
    favorited: bool,
    selected: bool,
    unavailable: bool,
}

export struct LibraryRowData {
    leading: string,
    primary: string,
    secondary: string,
    item_kind: int, // 0 track, 1 artist, 2 album, 3 genre, 4 decade
    album_art: image,
    has_album_art: bool,
    source_badge: string,
    is_playing: bool,
    favoritable: bool,
    favorited: bool,
    selected: bool,
}

export struct MetadataEditorField {
    id: string,
    field_name: string,
    value: string,
    common: bool,
}

